{
  "name": "optimai",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "test": "turbo test",
    "clean": "turbo clean && rm -rf node_modules",
    "cli": "pnpm -F @optimai/cli build && node apps/cli/dist/index.js",
    "cli:dev": "pnpm -F @optimai/cli dev"
  },
  "devDependencies": {
    "pg": "^8.17.2",
    "postgres": "^3.4.8",
    "turbo": "^2.3.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0"
  }
}
